<main>
    <h2>{{wingspots.name}}</h2>
    <div class="wingspots-container-show">
        <img src="{{wingspots.image}}" />
        <br>
        <h4>{{users.headline}}</h4>
        <p>
            <h2> {{wingspots.location}}</h2>
        </p>
        <h3>{{wingspots.name}}'s Favorite Wing Spots</h3>
        {{#each wingspots.name}}
        <a href="{{userId}}/wingspots/{{id}}">
            <h3>{{name}}</h3>
        </a>
        {{/each}}
    </div>
    <a href="/users/{{userId}}">Return to Users Page</a>
</main>
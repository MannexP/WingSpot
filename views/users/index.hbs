<main>
    <h1>
        WingSpot Afficioandos
    </h1>

    {{#each users}}
    <div class="card" style="width: 18rem;">
        <img class="card-img-top" src="{{image}}">
        <div class="card-body">
            <h1 class="card-title">{{name}}</h1>
            <h4 class="card-text">{{headline}}</h4>
            <a href="/users/{{_id}}" class="btn btn-dark">My Wing Spots</a>
        </div>
    </div>

    {{/each}}

    

</main>